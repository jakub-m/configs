imap jk <Esc>
nmap ; :

syntax on
colorscheme elflord

set number

set expandtab
set shiftwidth=4
set softtabstop=4
set smartindent

"set tabstop=4
"set shiftwidth=4
"set softtabstop=4
"set noexpandtab
"set autoindent

set hlsearch
set ignorecase
set smartcase
set incsearch

autocmd BufNewFile,BufRead *.go set autoindent noexpandtab tabstop=8 shiftwidth=8

